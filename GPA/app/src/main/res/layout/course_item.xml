<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent" android:layout_width="fill_parent"
	android:textSize="18sp">

	<TextView android:layout_height="fill_parent" android:id="@+id/crsetext"
		android:layout_width="wrap_content" android:paddingLeft="10sp"
		android:layout_alignParentStart="true"  android:textSize="18sp"
		android:paddingRight="10sp"
		android:layout_alignTop="@+id/creditstext">
	</TextView>
	<TextView android:layout_height="fill_parent" android:id="@+id/gradetext"
		android:layout_width="wrap_content" android:paddingLeft="10sp"
		android:paddingRight="10sp"  android:layout_toStartOf="@+id/creditstext"
		android:gravity="start"
		android:layout_alignTop="@+id/creditstext"  android:textSize="18sp">
	</TextView>
	<TextView android:id="@+id/creditstext" android:layout_width="wrap_content"
		android:layout_height="fill_parent" android:layout_alignParentEnd="true"
		android:paddingLeft="10sp" android:paddingRight="10sp"  android:textSize="18sp"
		android:layout_alignTop="@+id/creditstext" >
	</TextView>



</RelativeLayout>
